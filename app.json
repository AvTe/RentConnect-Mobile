{
  "expo": {
    "name": "Yoombaa",
    "slug": "yoombaa-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FE9200"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yoombaa.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FE9200"
      },
      "package": "com.yoombaa.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "yoombaa",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "yoombaa",
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "ba4a6761-e0c2-44d3-bff3-66575fdf9d0c"
      }
    }
  }
}
