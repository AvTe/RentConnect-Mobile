{
  "expo": {
    "name": "RentConnect",
    "slug": "rentconnect-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FE9200"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yoombaa.rentconnect"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FE9200"
      },
      "package": "com.yoombaa.rentconnect"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "rentconnect",
    "plugins": [
      "expo-font"
    ]
  }
}
